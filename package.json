{
  "name": "mcp-browser-control",
  "version": "1.0.0",
  "description": "MCP Server for browser automation using Selenium WebDriver",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "browser",
    "automation",
    "selenium",
    "webdriver",
    "testing"
  ],
  "author": "MCP Browser Control Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.0",
    "selenium-webdriver": "^4.18.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/selenium-webdriver": "^4.1.21",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
